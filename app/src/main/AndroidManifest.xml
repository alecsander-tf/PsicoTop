<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.psicotop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/CustomNoActionBar">
        <activity android:name="menu.detalhesResumo.DetalhesResumoActivity"
            android:parentActivityName="menu.MenuActivity" />
        <activity
            android:name="configuracoes.ConfiguracoesActivity"
            android:parentActivityName="menu.MenuActivity" />
        <activity android:name=".mvp.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="menu.MenuActivity"
            android:theme="@style/CustomNoActionBar"></activity>
        <activity
            android:name=".mvp.registro.RegistroActivity"
            android:parentActivityName=".mvp.login.LoginActivity" />
    </application>

</manifest>